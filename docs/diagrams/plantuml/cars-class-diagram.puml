@startuml

title
Cars - Class diagram
end title

skinparam class {
	BackgroundColor White
	ArrowColor DarkSlateGray
	BorderColor DarkSlateGray
}

class Car {
    + id : int
    + brand : String
    + model : String
}

class Owner {
    + id : int
    + name : String
}

class Dealership {
    + id : int
    + name : String
    + address : String
}

Car *- "1" Owner
Car "*" *-* "*" Dealership : sells

@enduml